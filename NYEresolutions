--Most Popular tweeted category
SELECT tweet_category, COUNT(*)
FROM dbo.NYEResolutions
GROUP BY tweet_category
ORDER BY COUNT (*) DESC

-- Least popular tweeted category
SELECT tweet_category, COUNT (*)
FROM dbo.NYEResolutions
GROUP BY tweet_category
ORDER BY COUNT (*) 

-- Category that was retweeted the most
SELECT tweet_category, retweet_count
FROM dbo.NYEResolutions
WHERE retweet_count = (SELECT MAX (retweet_count) FROM dbo.NYEResolutions) 

